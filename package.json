{
  "name": "vendure-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:vendure:server": "ts-node ./src/index.ts",
    "dev:vendure:worker": "ts-node ./src/index-worker.ts",
    "dev:vendure": "concurrently npm:dev:vendure:*",
    "build": "tsc",
    "start:vendure:server": "node ./dist/index.js",
    "start:vendure:worker": "node ./dist/index-worker.js",
    "start:vendure": "concurrently npm:start:vendure:*",
    "migration:generate": "ts-node migration generate",
    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "2.1.0",
    "@vendure/asset-server-plugin": "2.1.0",
    "@vendure/core": "2.1.0",
    "@vendure/email-plugin": "2.1.0",
    "better-sqlite3": "9.0.0",
    "dotenv": "16.3.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "concurrently": "8.2.1",
    "ts-node": "10.9.1"
  }
}
